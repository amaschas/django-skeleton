<uwsgi>
    <socket>127.0.0.1:6969</socket>
    <pythonpath>$$$$REMOTE_PROJECT_ROOT$$$$/current</pythonpath>
    <module>wsgi</module>
    <plugins>python</plugins>
    <processes>1</processes>
    <pidfile>/var/run/uwsgi/%n/pid</pidfile>
    <daemonize/>
    <uid>33</uid>
    <gid>33</gid>
    <enable-threads/>
    <master/>
    <harakiri>120</harakiri>
    <max-requests>5000</max-requests>
</uwsgi>
